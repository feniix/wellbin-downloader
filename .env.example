# Wellbin Medical Scraper Configuration
# Copy this file to .env and update with your credentials

# Wellbin Login Credentials
WELLBIN_EMAIL=your-email@example.com
WELLBIN_PASSWORD=your-password

# Wellbin Directories
WELLBIN_OUTPUT_DIR=medical_data_source
WELLBIN_MARKDOWN_DIR=medical_data_markdown

# Scraper Settings
WELLBIN_STUDY_LIMIT=
WELLBIN_STUDY_TYPES=FhirStudy
WELLBIN_HEADLESS=true

# PDF Converter Settings
WELLBIN_INPUT_DIR=medical_data_source
WELLBIN_PRESERVE_STRUCTURE=true
WELLBIN_FILE_TYPE=all
