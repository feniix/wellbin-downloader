{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp"
      ]
    },
    "mcp-pandoc": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-pandoc"
      ]
    },
    "sequential-thinking": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/arben-adm/mcp-sequential-thinking",
        "--with",
        "portalocker",
        "mcp-sequential-thinking"
      ]
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "/Users/feniix/src/personal/cursor/wellbin"
      ]
    },
    "exa": {
      "command": "direnv",
      "args": [
        "exec",
        "/Users/feniix/src/personal/cursor/kinemotion",
        "bunx", "-y", "exa-mcp-server"
      ]
    },
    "ref": {
      "command": "direnv",
      "args": [
        "exec",
        "/Users/feniix/src/personal/cursor/kinemotion",
        "bunx", "-y", "ref-tools-mcp@latest"
      ]
    }
  }
}
